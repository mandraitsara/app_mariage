{% extends 'base.html.twig' %}

{% block body %}
	<h1>Upload Images</h1>

	{{ form_start(form) }}
	{{ form_row(form.name) }}
	{{ form_row(form.image) }}
	{{ form_row(form.envoyer) }}
	{{ form_end(form) }}

	{% for message in app.flashes('success') %}
		<div class="alert alert-success">
			{{ message }}
		</div>
	{% endfor %}

	{% for message in app.flashes('error') %}
		<div class="alert alert-danger">
			{{ message }}
		</div>
	{% endfor %}
{% endblock %}
